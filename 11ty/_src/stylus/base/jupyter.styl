    &.cm-string
      color #BA2121
    &.cm-string-2
      color #f50
    &.cm-meta
      color #AA22FF
    &.cm-qualifier
      color #555
    &.cm-builtin
      color #008000
    &.cm-bracket
      color #997
    &.cm-tag
      color #170
    &.cm-attribute
      color #00c
    &.cm-header
      color blue
    &.cm-quote
