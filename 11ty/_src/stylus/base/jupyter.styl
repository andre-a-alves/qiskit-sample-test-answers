  color #000

.highlight-variable
  color #000

.highlight-variable-2
  color #1a1a1a

.highlight-variable-3
  color #333333

.highlight-string
  color #BA2121

.highlight-comment
  color #408080
  font-style italic

.highlight-number
  color #080

.highlight-atom
  color #88F

.highlight-keyword
  color #008000
  font-weight bold

.highlight-builtin
  color #008000

.highlight-error
