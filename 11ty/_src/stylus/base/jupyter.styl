/*
 * Converted from notebook.css found at https://github.com/cbernet/jupyter_web
 */
.prompt
  min-width 14ex
  padding 0.4em
  margin 0
  font-family monospace
  text-align left
  line-height 1.21429em
  -webkit-touch-callout none
  -webkit-user-select none
  -khtml-user-select none
  -moz-user-select none
  -ms-user-select none
  user-select none
  cursor default

@media (max-width: 540px)
  .prompt
    text-align left

div
  &.inner_cell
    min-width 0
    display -webkit-box
      -webkit-box-orient vertical
      -webkit-box-align stretch
    display -moz-box
      -moz-box-orient vertical
      -moz-box-align stretch
    display box
      box-orient vertical
      box-align stretch
    display flex
      flex-direction column
      align-items stretch
      -webkit-box-flex 1
      -moz-box-flex 1
      box-flex 1
      flex 1
  &.input_area
    border 1px solid #cfcfcf
    border-radius 2px
    background #f7f7f7
    line-height 1.21429em
    & > div
      &.highlight
        margin 0.4em
        border none
        padding 0
        background-color transparent
        & > pre
          margin 0
          border none
          padding 0
          background-color transparent
  &.prompt
    &:empty
      padding-top 0
      padding-bottom 0
  &.unrecognized_cell
    padding 5px 5px 5px 0
    display -webkit-box
      -webkit-box-orient horizontal
      -webkit-box-align stretch
    display -moz-box
      -moz-box-orient horizontal
      -moz-box-align stretch
    display box
      box-orient horizontal
      box-align stretch
    display flex
      flex-direction row
      align-items stretch
    .inner_cell
      border-radius 2px
      padding 5px
      font-weight bold
      color red
      border 1px solid #cfcfcf
      background #eaeaea
      a
        color inherit
        text-decoration none
        &:hover
          color inherit
          text-decoration none
  &.code_cell
  &.input
    page-break-inside avoid
    display -webkit-box
      -webkit-box-orient horizontal
      -webkit-box-align stretch
    display -moz-box
      -moz-box-orient horizontal
      -moz-box-align stretch
    display box
      box-orient horizontal
      box-align stretch
    display flex
      flex-direction row
      align-items stretch
  &.input_prompt
    color #303F9F
    border-top 1px solid transparent
  &.output_wrapper
    position relative
    display -webkit-box
      -webkit-box-orient vertical
      -webkit-box-align stretch
    display -moz-box
      -moz-box-orient vertical
      -moz-box-align stretch
    display box
      box-orient vertical
      box-align stretch
    display flex
      flex-direction column
      align-items stretch
      z-index 1
  &.output_scroll
    height 24em
    width 100%
    overflow auto
    border-radius 2px
    -webkit-box-shadow inset 0 2px 8px rgba(0, 0, 0, 0.8)
    box-shadow inset 0 2px 8px rgba(0, 0, 0, 0.8)
    display block
    div
      &.output_subarea
        overflow-x visible
  &.output_collapsed
    margin 0
    padding 0
    display -webkit-box
      -webkit-box-orient vertical
      -webkit-box-align stretch
    display -moz-box
      -moz-box-orient vertical
      -moz-box-align stretch
    display box
      box-orient vertical
      box-align stretch
    display flex
      flex-direction column
      align-items stretch
  &.out_prompt_overlay
    height 100%
    padding 0 0.4em
    position absolute
    border-radius 2px
    &:hover
      -webkit-box-shadow inset 0 0 1px #000
      box-shadow inset 0 0 1px #000
      background rgba(240, 240, 240, 0.5)
  &.output_prompt
    color #D84315
  &.output_area
    padding 0
    page-break-inside avoid
    display -webkit-box
      -webkit-box-orient horizontal
      -webkit-box-align stretch
    display -moz-box
      -moz-box-orient horizontal
      -moz-box-align stretch
    display box
      box-orient horizontal
      box-align stretch
    display flex
      flex-direction row
      align-items stretch
    .MathJax_Display
      text-align left !important
    .rendered_html
      table
        margin-left 0
        margin-right 0
      img
        margin-left 0
        margin-right 0
    .mglyph
      & > img
        max-width none
    pre
      max-height 500px
      margin 0
      padding 1px 0 1px 0
      border 0
      vertical-align baseline
      color black
      background-color transparent
      border-radius 0
  &.output_subarea
    overflow-x auto
    padding 0.4em
    -webkit-box-flex 1
    -moz-box-flex 1
    box-flex 1
    flex 1
    max-width calc(100% - 14ex)
  &.output_text
    text-align left
    color #000
    line-height 1.21429em
  &.output_stderr
    background #fdd
  &.output_latex
    text-align left
  &.output_javascript
    &:empty
      padding 0

@media (max-width: 540px)
  div
    &.unrecognized_cell
      & > div
        &.prompt
          display none

@media print
  div
    &.code_cell
      page-break-inside avoid

@media (max-width: 540px)
  div
    &.input
      display -webkit-box
        -webkit-box-orient vertical
        -webkit-box-align stretch
      display -moz-box
        -moz-box-orient vertical
        -moz-box-align stretch
      display box
        box-orient vertical
        box-align stretch
      display flex
        flex-direction column
        align-items stretch

.CodeMirror
  line-height 1.21429em
  font-size 14px
  height auto
  background none
  pre
    padding 0 0.4em
    border 0
    border-radius 0

.CodeMirror-scroll
  overflow-y hidden
  overflow-x auto

.CodeMirror-lines
  padding 0.4em 0

.CodeMirror-linenumber
  padding 0 8px 0 4px

.CodeMirror-gutters
  border-bottom-left-radius 2px
  border-top-left-radius 2px

.CodeMirror-cursor
  border-left 1.4px solid black

@media screen and (min-width: 2138px) and (max-width: 4319px)
  .CodeMirror-cursor
    border-left 2px solid black

@media screen and (min-width: 4320px)
  .CodeMirror-cursor
    border-left 4px solid black

