{
  "name": "11ty",
  "version": "0.1.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "watch:style": "stylus ./_src/stylus/ --compress --watch --out _src/static",
    "build:style": "stylus ./_src/stylus/main.styl --compress",
    "watch:11ty": "eleventy --serve",
    "build:11ty": "eleventy",
    "clean": "rm -r ./_site",
    "build": "npm run build:style && npm run build:11ty",
    "serve": "concurrently \"npm:watch:style\" \"npm:watch:11ty\""
  },
  "keywords": [],
  "author": "",
  "license": "GPL3",
  "devDependencies": {
    "@11ty/eleventy": "^2.0.1",
    "@11ty/eleventy-plugin-syntaxhighlight": "^5.0.0",
    "concurrently": "^8.0.1",
    "stylus": "^0.59.0"
  },
  "dependencies": {
    "mathjax": "^3.2.2",
    "purecss": "^3.0.0"
  }
}
